   "source": [
    "def is_symmetric_number(n):\n",
    "    s = str(n)\n",
    "    if s!= s[::-1]:\n",
    "        return False\n",
    "\n",
    "    return True\n",
    "\n",
    "print(is_symmetric_number(1234))\n",
    "print(is_symmetric_number(121))"
   ],
